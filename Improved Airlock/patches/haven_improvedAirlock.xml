<Patch>
  <Operation Class="AttributeSet">
    <!-- Airlock Oxygen -->
    <xpath>/data/Element/me[@mid="120"]/data/l/element/spaceSuitLocker[@maxOxygen="6000"]</xpath>
    <attribute>maxOxygen</attribute>
    <value>{airlockOxygen}</value>
  </Operation>
  <Operation Class="AttributeSet">
    <!-- Airlock Outgoing Inventory -->
    <xpath>/data/Element/me[@mid="120"]/data/l/element/dockingPort</xpath>
    <attribute>outGoingSize</attribute>
    <value>{airlockInventoryOut}</value>
  </Operation>
  <Operation Class="AttributeSet">
    <!-- Airlock Incoming Inventory -->
    <xpath>/data/Element/me[@mid="120"]/data/l/element/dockingPort</xpath>
    <attribute>incomingSize</attribute>
    <value>{airlockInventoryIn}</value>
  </Operation>
  <Operation Class="AttributeSet">
    <!-- Spacesuit Locker -->
    <xpath>/data/Element/me[@mid="200"]/data/l/element/spaceSuitLocker[@maxOxygen="6000"]</xpath>
    <attribute>maxOxygen</attribute>
    <value>{suitLockerOxygen}</value>
  </Operation>
  <Operation Class="AttributeSet">
    <!-- Airlock Doorway -->
    <xpath>/data/Element/me[@mid="992"]/data/l/element/spaceSuitLocker[@maxOxygen="1000"]</xpath>
    <attribute>maxOxygen</attribute>
    <value>{airlockDoorway}</value>
  </Operation>
</Patch>
